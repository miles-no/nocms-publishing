  },
  "homepage": "https://github.com/miles-no/nocms-publishing#readme",
  "dependencies": {
    "cloudinary-core": "^2.3.0",
    "moment": "^2.19.3",
    "nocms-ajax": "^1.0.0",
    "nocms-atoms": "^1.0.2",
    "nocms-events": "^1.2.0",
    "nocms-forms": "^3.6.3",
    "nocms-image-cropper": "^1.1.0",
    "nocms-shortcuts": "^1.0.1",
    "nocms-utils": "^1.0.0",
    "prop-types": "^15.6.0",
