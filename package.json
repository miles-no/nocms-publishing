  "description": "Publisher interface for NoCMS",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "npm run build-js && npm run build-css",
    "eslint": "eslint --ext .js, .jsx .",
